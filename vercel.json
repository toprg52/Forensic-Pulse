{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/index.py": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "^/api(/.*)?$",
      "dest": "/api/index.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    },
    {
      "src": "^/(?!api).*$",
      "dest": "/index.html"
    }
  ]
}
